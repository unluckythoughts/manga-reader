{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}"
    },
    {
      "name": "Launch Test file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/tmp/test.go"
    },
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "env": {
        "WEB_PORT": "5678",
        "DB_FILE_PATH": "db.sqlite",
        "WEB_CORS": "true",
        "WEB_PROXY": "true",
        "DB_DEBUG": "true",
      },
      "program": "${workspaceFolder}/main.go"
    }
  ]
}